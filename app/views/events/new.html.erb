<h1> Create Events</h1>
<%= form_for @event do |f|%>
  <%= f.label :date%><br>
  <%= f.datetime_field :date%>
  <%= f.label :location%><br>
  <%= f.text_field :location%><br>
   <%= f.label :title%><br>
  <%= f.text_field :title%><br>
   <%= f.label :description%><br>
  <%= f.text_area :description%><br>
  <%= f.submit :submit, class: 'btn btn-primary btn-sm'%>
<% end %>
